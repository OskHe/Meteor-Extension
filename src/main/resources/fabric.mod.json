{
  "schemaVersion": 1,
  "id": "meteor-extension",
  "version": "${version}",
  "name": "Meteor-Extension",
  "description": "An extension for the meteor-client.",
  "authors": [
    "OskHe"
  ],
  "contact": {
    "repo": "https://github.com/oskhe/meteor-extension"
  },
  "icon": "assets/meteorextension/iconOH.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.oskhe.meteorextension.MeteorExtension"
    ]
  },
  "mixins": [
    "meteorextension.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "25,255,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
